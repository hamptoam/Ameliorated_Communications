@model IEnumerable<Ameliorated_Communications.Models.Fund>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DailyFunds)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WeeklyFunds)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MonthlyFunds)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QuarterlyFunds)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.YearlyFunds)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DailyFunds)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WeeklyFunds)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MonthlyFunds)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QuarterlyFunds)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.YearlyFunds)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FundId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FundId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FundId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
